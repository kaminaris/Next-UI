        <script>
    var field = $("#served");

    function getRequestStats() {
        $.ajax({
            url: "/requests/stats?r=" + Math.floor(Math.random() * 99999999),
            success: function(response) {
                var amount = String(response.tfhour).replace(/(.)(?=(\d{3})+$)/g,'$1,');
                field.text(amount);

                // self call
                setTimeout(function() {
                    getRequestStats();
                }, 2000)
            }
        })
    }

    // run
    getRequestStats();

</script>
